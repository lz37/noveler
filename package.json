{
	"name": "noveler",
	"description": "Sample for the decorator API",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"license": "MIT",
	"engines": {
		"vscode": "^1.71.2"
	},
	"repository": {
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"colors": [
			{
				"id": "number",
				"description": "数字的颜色",
				"defaults": {
					"dark": "#add8e6",
					"light": "#00008b"
				}
			},
			{
				"id": "bookTitleMark",
				"description": "书名号的颜色",
				"defaults": {
					"dark": "#d3090a",
					"light": "#a83f3f"
				}
			},
			{
				"id": "quote",
				"description": "引号的颜色",
				"defaults": {
					"dark": "#90ee90",
					"light": "#006400"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.13.0",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"@types/lodash": "^4.14.186",
		"lodash": "^4.17.21"
	}
}
