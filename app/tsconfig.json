{
	"compilerOptions": {
		"module": "ESNext",
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"target": "ES5",
		"jsx": "react",
		"sourceMap": true,
		"experimentalDecorators": true,
		"lib": ["dom", "ES2015"],
		"strict": true,
		"skipLibCheck":true
	},
	"exclude": ["node_modules"]
}
